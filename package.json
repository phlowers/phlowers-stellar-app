{
  "name": "phlowers-stellar-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --port 4200",
    "build": "ng build --configuration=production",
    "build:fr": "npm run set-version-and-build-time && ng build --output-mode=static --configuration=production,fr && uv run --no-cache ./scripts/create_assets_list_for_service_worker.py --language fr",
    "build:en": "npm run set-version-and-build-time && ng build --output-mode=static --configuration=production,en && uv run --no-cache ./scripts/create_assets_list_for_service_worker.py --language en",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "prettier-check": "prettier --check \"**/*.{js,mjs,ts,mts,d.ts,html}\"",
    "lint-check": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom 'src/**/*.ts'",
    "set-up-mechaphlowers": "uv run ./scripts/set_up_mechaphlowers.py",
    "set-version-and-build-time": "uv run ./scripts/set-version-and-build-time.py",
    "create-mock-data": "uv run ./scripts/create_mock_data.py",
    "create-assets-list-for-service-worker:fr": "uv run  --no-cache ./scripts/create_assets_list_for_service_worker.py --language fr",
    "create-assets-list-for-service-worker:en": "uv run  --no-cache ./scripts/create_assets_list_for_service_worker.py --language en",
    "extract-i18n": "ng extract-i18n --output-path assets/i18n --format=xlf2"
  },
  "browser": {
    "child_process": false,
    "crypto": false,
    "fs": false,
    "fs/promises": false,
    "path": false,
    "url": false,
    "vm": false,
    "ws": false,
    "node:crypto": false,
    "node:url": false
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-slider/ngx-slider": "^19.0.0",
    "@angular/animations": "19.2.4",
    "@angular/common": "19.2.4",
    "@angular/compiler": "19.2.4",
    "@angular/core": "19.2.4",
    "@angular/forms": "19.2.4",
    "@angular/platform-browser": "19.2.4",
    "@angular/platform-browser-dynamic": "19.2.4",
    "@angular/router": "19.2.4",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@primeng/themes": "19.0.10",
    "@types/plotly.js-dist-min": "^2.3.4",
    "chart.js": "4.4.2",
    "dexie": "^4.0.11",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "esbuild-plugins-node-modules-polyfill": "^1.7.0",
    "font-awesome": "^4.7.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "lodash": "^4.17.21",
    "plotly.js-dist-min": "^3.0.1",
    "primeclt": "^0.1.5",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.8",
    "pyodide": "0.27.4",
    "raw-loader": "^4.0.2",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.5.1",
    "ts-jest-mock-import-meta": "^1.2.1",
    "tslib": "^2.3.0",
    "url-browserify": "^1.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.14.0",
    "@angular-builders/custom-esbuild": "19.1.0",
    "@angular-builders/custom-webpack": "19.0.1",
    "@angular-devkit/build-angular": "19.2.4",
    "@angular/cli": "19.2.4",
    "@angular/compiler-cli": "19.2.4",
    "@angular/localize": "19.2.4",
    "@pyodide/webpack-plugin": "^1.3.3",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/plotly.js": "^2.35.5",
    "angular-eslint": "19.3.0",
    "autoprefixer": "^10.4.20",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~5.4.0",
    "jest-raw-loader": "^1.0.1",
    "jsdom": "^26.0.0",
    "ng-extract-i18n-merge": "^2.14.3",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.28.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.27.0"
  }
}
