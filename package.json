{
  "name": "phlowers-stellar-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build:service-worker": "tsc -p tsconfig.worker.json && sed -i 's/export //g' public/service-worker.js",
    "build": "npm run build:service-worker && ng build --configuration=production",
    "build:fr": "npm run build:service-worker && ng build --output-mode=static --configuration=production,fr && npm run set-env-variables && npm run create-assets-list-for-service-worker:fr",
    "build:en": "npm run build:service-worker && ng build --output-mode=static --configuration=production,en && npm run set-env-variables && npm run create-assets-list-for-service-worker:en",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "prettier-check": "prettier --check \"**/*.{js,mjs,ts,mts,d.ts,html}\"",
    "lint-check": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom 'src/**/*.ts'",
    "set-up-mechaphlowers": "uv run --prerelease=allow ./scripts/set_up_mechaphlowers.py",
    "set-env-variables": "uv run ./scripts/set-env-variables.py",
    "create-mock-data": "uv run ./scripts/create_mock_data.py",
    "create-assets-list-for-service-worker:fr": "uv run  --no-cache ./scripts/create_assets_list_for_service_worker.py --language fr",
    "create-assets-list-for-service-worker:en": "uv run  --no-cache ./scripts/create_assets_list_for_service_worker.py --language en",
    "extract-i18n": "ng extract-i18n --output-path assets/i18n --format=xlf2",
    "cyclonedx": "npx @cyclonedx/cyclonedx-npm --output-file bom.xml  --output-format XML"
  },
  "browser": {
    "child_process": false,
    "crypto": false,
    "fs": false,
    "fs/promises": false,
    "path": false,
    "url": false,
    "vm": false,
    "ws": false,
    "node:crypto": false,
    "node:url": false
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-slider/ngx-slider": "^20.0.0",
    "@angular/animations": "20.3.9",
    "@angular/common": "20.3.9",
    "@angular/compiler": "20.3.9",
    "@angular/core": "20.3.9",
    "@angular/forms": "20.3.9",
    "@angular/platform-browser": "20.3.9",
    "@angular/platform-browser-dynamic": "20.3.9",
    "@angular/router": "20.3.9",
    "@eslint/js": "^9.27.0",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@primeng/themes": "20.3.0",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@typescript-eslint/parser": "^8.32.1",
    "chart.js": "4.4.2",
    "dexie": "^4.0.11",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "esbuild-plugins-node-modules-polyfill": "^1.7.0",
    "font-awesome": "^4.7.0",
    "javascript-time-ago": "^2.5.11",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "lodash": "^4.17.21",
    "marked": "15.0.12",
    "ngx-markdown": "20.1.0",
    "papaparse": "5.5.3",
    "plotly.js-dist-min": "^3.0.1",
    "primeclt": "^0.1.5",
    "primeicons": "^7.0.0",
    "primeng": "20.3.0",
    "pyodide": "0.27.4",
    "raw-loader": "^4.0.2",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.5.1",
    "ts-jest-mock-import-meta": "^1.2.1",
    "tslib": "^2.3.0",
    "url-browserify": "^1.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^2.1.2",
    "@angular-builders/custom-esbuild": "20.0.0",
    "@angular-devkit/build-angular": "20.3.9",
    "@angular/cli": "20.3.9",
    "@angular/compiler-cli": "20.3.9",
    "@angular/localize": "20.3.9",
    "@cyclonedx/cyclonedx-npm": "^3.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/papaparse": "5.3.16",
    "@types/plotly.js": "^2.35.5",
    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.20",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~5.4.0",
    "jest-raw-loader": "^1.0.1",
    "jest-sonar": "^0.2.16",
    "jsdom": "^26.0.0",
    "ng-extract-i18n-merge": "~3.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "random-words-commonjs": "^2.0.1",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.28.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.48.1"
  }
}
